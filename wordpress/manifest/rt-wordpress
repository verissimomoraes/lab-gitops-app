kind: Route
apiVersion: route.openshift.io/v1
metadata:
  name: rt-wordpress
  namespace: gitops-app
  annotations:
    openshift.io/host.generated: 'true'
spec:
  host: wordpress-dev.apps.bbassetnonprod.brazilsouth.aroapp.io
  to:
    kind: Service
    name: wordpress-svc-clusterip
    weight: 100
  port:
    targetPort: 8080
  wildcardPolicy: None
